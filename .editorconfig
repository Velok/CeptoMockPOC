root = true

[*]
indent_style = space
indent_size = 2
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
indent_size = 4
charset = utf-8-bom

[*.sln]
indent_style = tab
indent_size = 4

[*.json]
insert_final_newline = false

[*.DotSettings]
indent_style = tab
indent_size = 4
end_of_line = lf
insert_final_newline = false